    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^18.6.2",
    "@types/prettier": "^2.6.4",
    "@types/react": "^18.0.15",
    "@types/ws": "^8.5.3",
    "@unocss/autocomplete": "workspace:*",
