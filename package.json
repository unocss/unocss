    "@iconify-json/tabler": "catalog:icons",
    "@iconify-json/teenyicons": "catalog:icons",
    "@iconify-json/twemoji": "catalog:icons",
    "@iconify-json/uim": "catalog:icons",
    "@shikijs/vitepress-twoslash": "catalog:docs",
