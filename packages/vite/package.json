{
  "name": "@unocss/vite",
  "type": "module",
  "version": "0.61.0",
  "description": "The Vite plugin for UnoCSS",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/unocss/unocss/tree/main/packages/vite#readme",
  "repository": {
