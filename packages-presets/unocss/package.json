{
  "name": "unocss",
  "type": "module",
  "version": "66.6.2",
  "description": "The instant on-demand Atomic CSS engine.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://unocss.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unocss/unocss.git",
    "directory": "packages-presets/unocss"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/antfu"
  },
  "bugs": {
    "url": "https://github.com/unocss/unocss/issues"
  },
  "keywords": [
    "unocss",
    "atomic-css",
    "atomic-css-engine",
    "css",
    "tailwind",
    "windicss"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./astro": "./dist/astro.mjs",
    "./postcss": {
      "import": "./dist/postcss.mjs",
      "require": "./dist/postcss.cjs"
    },
    "./preset-attributify": "./dist/preset-attributify.mjs",
    "./preset-icons": "./dist/preset-icons.mjs",
    "./preset-mini": "./dist/preset-mini.mjs",
    "./preset-tagify": "./dist/preset-tagify.mjs",
    "./preset-typography": "./dist/preset-typography.mjs",
    "./preset-uno": "./dist/preset-uno.mjs",
    "./preset-web-fonts": "./dist/preset-web-fonts.mjs",
    "./preset-wind": "./dist/preset-wind.mjs",
    "./preset-wind3": "./dist/preset-wind3.mjs",
    "./preset-wind4": "./dist/preset-wind4.mjs",
    "./vite": "./dist/vite.mjs",
    "./webpack": {
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.mts",
        "./dist/*.d.cts",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch"
  },
  "peerDependencies": {
    "@unocss/astro": "workspace:*",
    "@unocss/postcss": "workspace:*",
    "@unocss/webpack": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@unocss/astro": {
      "optional": true
    },
    "@unocss/postcss": {
      "optional": true
    },
    "@unocss/webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@unocss/cli": "workspace:*",
    "@unocss/core": "workspace:*",
    "@unocss/preset-attributify": "workspace:*",
    "@unocss/preset-icons": "workspace:*",
    "@unocss/preset-mini": "workspace:*",
    "@unocss/preset-tagify": "workspace:*",
    "@unocss/preset-typography": "workspace:*",
    "@unocss/preset-uno": "workspace:*",
    "@unocss/preset-web-fonts": "workspace:*",
    "@unocss/preset-wind": "workspace:*",
    "@unocss/preset-wind3": "workspace:*",
    "@unocss/preset-wind4": "workspace:*",
    "@unocss/transformer-attributify-jsx": "workspace:*",
    "@unocss/transformer-compile-class": "workspace:*",
    "@unocss/transformer-directives": "workspace:*",
    "@unocss/transformer-variant-group": "workspace:*",
    "@unocss/vite": "workspace:*"
  },
  "devDependencies": {
    "@unocss/astro": "workspace:*",
    "@unocss/postcss": "workspace:*",
    "@unocss/webpack": "workspace:*"
  }
}
