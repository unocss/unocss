{
  "name": "@unocss/extractor-pug",
  "type": "module",
  "version": "66.5.12",
  "description": "UnoCSS extractor for Pug",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
