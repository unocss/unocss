{
  "name": "@unocss/inspector",
  "type": "module",
  "version": "66.3.2",
  "description": "The inspector UI for UnoCSS",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://unocss.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unocss/unocss.git",
    "directory": "packages-integrations/inspector"
  },
  "bugs": {
    "url": "https://github.com/unocss/unocss/issues"
  },
  "keywords": [
    "unocss",
    "inspector",
    "debugger"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "build-post": "vite build",
    "stub": "unbuild --stub",
    "dev": "nr stub && vite",
    "update-post": "vite build",
    "test:attw": "attw --pack --config-path ../../.attw-esm-only.json"
  },
  "dependencies": {
    "@unocss/core": "workspace:*",
    "@unocss/rule-utils": "workspace:*",
    "colorette": "catalog:",
    "gzip-size": "catalog:",
    "sirv": "catalog:",
    "vue-flow-layout": "catalog:"
  }
}
